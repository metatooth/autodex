{
  "name": "autodex",
  "version": "0.1.0",
  "description": "An aUTOmated inDEX",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && tsc -b ./src && npm run copy-files",
    "copy-files": "cp -r ./src/public/ ./dist/",
    "clean": "rm -rf ./dist",
    "etl": "tsc-watch --noClear -b ./src --onSuccess \"node dist/etl.js\"",
    "server": "tsc-watch --noClear -b ./src --onSuccess \"node dist/server.js\"",
    "client": "tsc-watch --noClear -b ./src --onSuccess \"node dist/client.js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fast-csv/parse": "^4.3.6",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "pg": "^8.7.3",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.25",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.3"
  }
}
